<div>

	<!--  第一部分：菜单导航 start -->
	<div class="row wrapper border-bottom white-bg page-heading">
		<div class="col-lg-10">
			<ol class="breadcrumb">
				<li>移动卡</li>
				<li class="active"><strong>库存卡管理</strong></li>
			</ol>
		</div>
		<div class="col-lg-2"></div>
	</div>
	<!--  第一部分：菜单导航 end -->
	

	<!--  第二部分：列表部分 start -->
	<div class="row wrapper wrapper-content animated fadeInRight advance-wrapper of-visible">
		<div class="ibox relative">
		
			<!--  搜索部分 start -->
			<div class="ibox-title clearfix">
				<div class="row">
					<div class="col-xs-8">
					
						<!--  关键字搜索部分 start -->
						<form class="form-inline" role="form" ng-submit="lifeCycleSearchForm();">
                            <div class="input-group">
                                <input type="text" class="form-control w-250" ng-model="lifeCycleQuery.queryText" placeholder="请输入网卡号、ICCID、IMSI" ng-disabled="advanceExpand"/>
                                <span class="input-group-addon glyphicon glyphicon-remove" ng-show="advanceExpands" ng-click="toggleClose();" aria-hidden="true"></span>
                            </div>
                            <div class="input-group">
                                <button class="btn btn-primary" ng-disabled="advanceExpands" type="submit">搜索</button>
                            </div>
                            <div class="input-group">
                                <button type="button" class="btn btn-primary" ng-click="toggleAdvance();" ng-class="{active:advanceExpands}">高级搜索</button>
                            </div>
                        </form>
                        <!--  关键字搜索部分 end -->
                        
                        <!--  高级搜索部分 start -->
						<div class="advance" ng-show="advanceExpandShow">
							<div class="ibox">
								<div class="ibox-title">
									<h5>高级搜索</h5>
								</div>
								<div class="ibox-content">
									<form class="form-horizontal form-fixed form-fixed-6" name="advance_form" role="form">
										<div class="form-group">
                                            <label class="col-sm-2 control-label">ICCID:</label>
                                            <div class="control-content">
                                                <div class="row">
                                                    <div class="col-sm-5">
                                                        <input type="text" class="form-control" ng-model="lifeCycleQuery.iccid">
                                                    </div>
                                                    <label class="col-sm-2 control-label">IMSI:</label>
                                                    <div class="col-sm-5">
                                                        <input type="text" class="form-control" ng-model="lifeCycleQuery.imsi">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label">网卡号:</label>
                                            <div class="control-content">
                                                <div class="row">
                                                    <div class="col-sm-5">
                                                        <input type="text" class="form-control" ng-model="lifeCycleQuery.cardNo">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="hr-line-dashed"></div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label">卡类型:</label>
                                            <div class="control-content">
                                                <div class="row">
                                                    <div class="col-sm-5">
                                                        <ui-select ng-model="lifeCycleQuery.cardType" theme="bootstrap">
								                            <ui-select-match placeholder="全部">{{$select.selected.text}}</ui-select-match>
								                            <ui-select-choices repeat="item in cardTypeList">
								                                <div ng-bind=" item.text "></div>
								                            </ui-select-choices>
								                        </ui-select>
                                                    </div>
                                                    <label class="col-sm-2 control-label">卡状态:</label>
                                                    <div class="col-sm-5">
                                                        <ui-select ng-model="lifeCycleQuery.status" theme="bootstrap">
								                            <ui-select-match placeholder="全部">{{$select.selected.text}}</ui-select-match>
								                            <ui-select-choices repeat="item in statusList">
								                                <div ng-bind=" item.text "></div>
								                            </ui-select-choices>
								                        </ui-select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label">运营商名称:</label>
                                            <div class="control-content">
                                                <div class="row">
                                                    <div class="col-sm-5">
                                                        <ui-select ng-model="lifeCycleQuery.opId" theme="bootstrap">
								                            <ui-select-match placeholder="全部">{{$select.selected.text}}</ui-select-match>
								                            <ui-select-choices repeat="item in opList">
								                                <div ng-bind=" item.text "></div>
								                            </ui-select-choices>
								                        </ui-select>
                                                    </div>
                                                    <label class="col-sm-2 control-label">开户时间:</label>
                                                     <div class="col-sm-5">
                                                        <input date-range-picker name="openTime"
															class="form-control date-picker" type="text"
															ng-model="lifeCycleQuery.openTime" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
									</form>
								</div>
								<div class="ibox-footer clearfix">
									<button type="button" class="btn btn-default pull-left" ng-click="resetForm();">清空</button>
									<div class="pull-right">
										<button type="button" class="btn btn-primary" ng-click="lifeCycleHighSearchForm();">搜索</button>
										<button type="button" class="btn btn-default" ng-click="toggleClose();">取消</button>
									</div>
								</div>
							</div>
						</div>
						<!--  高级搜索部分 end -->
					</div>
				</div>
			</div>
			<!--  搜索部分 end -->
			

			<!--  表格部分 start -->
			<div class="ibox-content">
				<div process-datatable dt-options="dtOptions" dt-instance="dtInstance" dt-columns="dtColumns"/>
			</div>
			<!--  表格部分 end -->
			
		</div>
	</div>
	<!--  第二部分：列表部分 end -->
	
</div>

