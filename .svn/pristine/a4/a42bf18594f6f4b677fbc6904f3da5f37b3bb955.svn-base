<div class="inmodal">
	<div class="modal-header">
		<h4 class="modal-title">迁移</h4>
	</div>
	<div class="modal-body">
		<form role="form" class="form-horizontal" name="frm">
			<div>
				<table
					class="table table-striped table-bordered table-hover">
					<thead>
						<tr>
							<th width="100">旧ICCID:</th>
							<th width="100">{{transferQuery.srcIccid}}</th>
							<th width="100">新ICCID</th>
							<th width="100">{{transferQuery.dstIccid}}</th>
						</tr>
						<tr>
							<th width="100">旧IMSI:</th>
							<th width="100">{{transferQuery.srcImsi}}</th>
							<th width="100">新IMSI:</th>
							<th width="100">{{transferQuery.dstImsi}}</th>
						</tr>
						<tr>
							<th width="100">旧网卡号</th>
							<th width="100">{{transferQuery.srcCardNo}}</th>
							<th width="100">新网卡号</th>
							<th width="100">{{transferQuery.dstCardNo}}</th>
						</tr>
						<tr>
							<th width="100">来源:</th>
							<th width="100" >{{transferQuery.source}}</th>
							<th width="100">商户名称:</th>
							<th width="100">{{transferQuery.spName}}</th>
						</tr>
						<tr>
							<th width="100">迁移原因:</th>
							<th width="100">{{transferQuery.transReason}}</th>
							<th width="100">请求时间:</th>
							<th width="100">{{transferQuery.requestTime}}</th>
						</tr>
					</thead>
					
				</table>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button type="button" class="btn btn-primary"
			ng-click="TransferOp();">确认迁移</button>
		<button type="button" class="btn btn-white" ng-click="cancel();">取消迁移</button>
	</div>
</div>
