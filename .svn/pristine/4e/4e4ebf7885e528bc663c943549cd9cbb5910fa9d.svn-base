<div class="inmodal">
	<div class="modal-header">
		<h4 class="modal-title">回收</h4>
	</div>
	<div class="modal-body">
		<form role="form" class="form-horizontal" name="frm">
			<div>
				<table
					class="table table-striped table-bordered table-hover">
					<thead>
						<tr>
							<th width="100">ICCID:</th>
							<th width="100">{{recycleQuery.iccid}}</th>
							<th width="100">IMSI</th>
							<th width="100">{{recycleQuery.imsi}}</th>
						</tr>
						<tr>
							<th width="100">网卡号:</th>
							<th width="100">{{recycleQuery.cardNo}}</th>
							<th width="100">运营商类型:</th>
							<th width="100">{{recycleQuery.opName}}</th>
						</tr>
						<tr>
							<th width="100">卡状态</th>
							<th width="100"><font color="red">{{recycleQuery.cardStatus}}</font></th>
							<th width="100">套餐状态</th>
							<th width="100"><font color="red">{{recycleQuery.packageStatus}}</font></th>
						</tr>
						<tr>
							<th width="100">总流量(MB):</th>
							<th width="100" >{{recycleQuery.totalflow}}</th>
							<th width="100">剩余流量(MB)</th>
							<th width="100">{{recycleQuery.remainTotalflow}}</th>
						</tr>
						<tr>
							<th width="100">套餐开始时间:</th>
							<th width="100">{{recycleQuery.packageBegintime}}</th>
							<th width="100">套餐结束时间:</th>
							<th width="100">{{recycleQuery.packageEndtime}}</th>
						</tr>
						<tr>
							<th width="100">来源:</th>
							<th width="100">{{recycleQuery.source}}</th>
							<th width="100">服务商名称</th>
							<th width="100">{{recycleQuery.spName}}</th>
						</tr>
						<tr>
							<th width="100">回收原因:</th>
							<th width="100">{{recycleQuery.recycleReason}}</th>
							<th width="100">请求时间:</th>
							<th width="100">{{recycleQuery.requestTime}}</th>
						</tr>
					</thead>
					
				</table>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button type="button" class="btn btn-primary"
			ng-click="RecycleOp();">确认回收</button>
		<button type="button" class="btn btn-white" ng-click="cancel();">取消回收</button>
	</div>
</div>
